{
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/socket.io/",
        "dest": "/app.js"
      },
      {
        "src": "/(.*)",
        "dest": "/app.js"
      }
    ]
  }
  